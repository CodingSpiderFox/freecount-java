<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <!--
        Added the constraints for entity BillPosition.
    -->
    <changeSet id="20211001175540-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="bill_id"
                                 baseTableName="bill_position"
                                 constraintName="fk_bill_position__bill_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bill"/>

        <addForeignKeyConstraint baseColumnNames="id"
                                 baseTableName="bill_position"
                                 constraintName="fk_bill_position__product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>
    </changeSet>
</databaseChangeLog>
